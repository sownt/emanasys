<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="1275.0" stylesheets="@../styles/main.css" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vosxvo.controllers.SalariesController">
   <children>
      <TableView fx:id="table" prefHeight="650.0" prefWidth="1275.0">
         <columns>
            <TableColumn fx:id="id" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" resizable="false" text="ID" />
            <TableColumn fx:id="salary" maxWidth="350.0" minWidth="350.0" prefWidth="350.0" resizable="false" text="SALARY" />
            <TableColumn fx:id="from" maxWidth="100.0" minWidth="400.0" prefWidth="100.0" resizable="false" text="FROM" />
            <TableColumn fx:id="to" maxWidth="400.0" minWidth="400.0" prefWidth="400.0" resizable="false" text="TO" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
